<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

  <item
    android:state_checked="false"
    android:bottom="5dp"
    android:left="5dp"
    android:right="5dp"
    android:top="5dp">
    <shape android:shape="oval">
      <solid android:color="@color/norbr_sdk_color_active" />
      <size
        android:width="12dp"
        android:height="12dp" />
      <stroke
        android:width="1sp"
        android:color="@color/black" />

    </shape>
  </item>
  <item
    android:state_checked="true"
    android:bottom="5dp"
    android:left="5dp"
    android:right="5dp"
    android:top="5dp">
    <shape android:shape="oval">
      <solid android:color="#0A4361" />
      <size
        android:width="12dp"
        android:height="12dp" />
      <stroke
        android:width="1sp"
        android:color="@color/black" />

    </shape>
  </item>

</selector>